    "test": "deno test --allow-read --allow-write"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/fs": "jsr:@std/fs@^1.0.19",
    "@std/path": "jsr:@std/path@^1.1.1",
    "@types/node": "npm:@types/node@^24.0.14",
    "zod": "npm:zod@^4.0.5"
  },
  "compilerOptions": {
    "lib": [
      "deno.ns",
      "deno.window"
    ]
  }
}
